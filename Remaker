local function showNotification()
    game.StarterGui:SetCore("SendNotification", {Title = "Fsploit Spam on/off", Text = "Turned on", Duration = 2.1})
end

button.MouseButton1Click:Connect(function()
    toggle()
    showNotification()
end)
-- end
end)


